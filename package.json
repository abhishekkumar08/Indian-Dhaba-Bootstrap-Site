{
  "name": "confusion",
  "version": "1.0.0",
  "description": "This is a website for The Indian Dhaba",
  "main": "index.html",
  "homepage": "http://abhishekkumar08.github.io/my-app",
  "scripts": {
    "start": "npm run lite",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "clean": "rimraf public",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* public/fonts",
    "imagemin": "imagemin img/* --out-dir='public/img'",
    "usemin": "usemin contactus.html -d public --htmlmin -o public/contactus.html && usemin aboutus.html -d public --htmlmin -o public/aboutus.html && usemin index.html -d public --htmlmin -o public/index.html",
    "build": "npm run clean && npm run imagemin && npm run copyfonts && npm run usemin",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "watch:all": "parallelshell 'npm run watch:scss' 'npm run lite'"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "cssmin": "^0.4.3",
    "gh-pages": "^3.1.0",
    "htmlmin": "0.0.7",
    "imagemin-cli": "^6.0.0",
    "less": "^4.1.1",
    "lite-server": "^2.6.1",
    "node-sass": "^5.0.0",
    "onchange": "^7.1.0",
    "parallelshell": "^3.0.2",
    "rimraf": "^3.0.2",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  }
}